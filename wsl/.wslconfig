[wsl2]
autoProxy=true
debugConsole=false
defaultVhdSize=64GB
dnsProxy=true
# dnsTunneling=true # disable to get access using VPN in windows
firewall=false
guiApplications=false
localhostForwarding=true
memory=12GB
nestedVirtualization=true
# networkingMode=mirrored # disable to get access using VPN in windows
swap=0

[experimental]
autoMemoryReclaim=dropcache
bestEffortDnsParsing=true
hostAddressLoopback=true
ignoredPorts=53,80
sparseVhd=true
