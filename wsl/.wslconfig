[wsl2]
# autoProxy=true            # default: true
# debugConsole=false        # default: false
# dnsProxy=true             # default: true
# dnsTunneling=true         # default: true # disable to get access using VPN in windows
# guiApplications=true      # default: true
# localhostForwarding=true  # default: true
# nestedVirtualization=true # default: true
# networkingMode=mirrored   # default: NAT # disable to get access using VPN in windows
# safeMode=false            # default: false
defaultVhdSize=64GB
firewall=false              # default: true
memory=12GB
swap=0

[experimental]
# bestEffortDnsParsing=true # default: false
# hostAddressLoopback=true  # default: false
autoMemoryReclaim=dropcache # default: disabled
ignoredPorts=22             # Specifies which ports Linux applications can bind to, even if that port is used in Windows.
sparseVhd=true              # default: false
