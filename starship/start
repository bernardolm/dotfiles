#!/usr/bin/env zsh

. "${DOTFILES}/starship/install"

# export STARSHIP_CONFIG="${DOTFILES}/starship/theme/tokyo-night.toml"
export STARSHIP_CONFIG="${DOTFILES}/starship/theme/pastel-powerline.toml"
# export STARSHIP_CONFIG="${DOTFILES}/starship/theme/starship.toml.toml"

