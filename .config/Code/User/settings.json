// Place your settings in this file to overwrite the default settings
{
    "[dockerfile]": {
        "editor.defaultFormatter": null,
    },
    "[go]": {
        "editor.formatOnPaste": true,
        "editor.formatOnSave": true,
        "editor.formatOnType": true,
    },
    "[javascript]": {
        "editor.defaultFormatter": "vscode.json-language-features",
        "editor.tabSize": 2
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features",
        "editor.tabSize": 2
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features",
        "editor.tabSize": 2
    },
    "[php]": {
        "editor.defaultFormatter": "bmewburn.vscode-intelephense-client"
    },
    "[python]": {
        "editor.formatOnPaste": true,
        "editor.formatOnSave": true,
    },
    "[xml]": {},
    "dlvLoadConfig": {
        "maxVariableRecurse": 3,
        "maxStringLen": 1024,
        "maxArrayValues": 400,
        "maxStructFields": -1
    },
    "editor.rulers": [
        79,
        119
    ],
    "files.associations": {
        "*.migration": "sql",
        ".env": "dotenv",
        ".zsh_history": "shellscript"
    },
    "files.watcherExclude": {
        "**/.git": true,
        "**/*assets*": true,
        "**/*zsh_history*": true,
        "**/bower_components": true,
        "**/node_modules": true,
        "**/vendor/**": true,
        "**/vendor/github.com/hurbcom": false,
        "**/venv/**": true,
    },
    "github.copilot.enable": {
        "*": true,
        "yaml": true,
        "plaintext": true,
        "markdown": false
    },
    "gitlens.advanced.messages": {
        "suppressLineUncommittedWarning": true
    },
    "go.editorContextMenuCommands": {
        "removeTags": true,
        "fillStruct": true,
        "testFile": true,
        "testPackage": true,
        "generateTestForFile": true,
        "generateTestForPackage": true,
        "benchmarkAtCursor": true
    },
    "go.delveConfig": {
        "dlvLoadConfig": {
            "maxStringLen": 1024,
        },
        "apiVersion": 2,
    },
    "go.formatFlags": [
        "-w"
    ],
    "go.liveErrors": {
        "enabled": true,
        "delay": 500
    },
    "go.testFlags": [
        "-short",
        "-race"
    ],
    "gopls": {
        "experimentalWorkspaceModule": true
    },
    "python.testing.pytestArgs": [
        "tests"
    ],
    "search.exclude": {
        "**/.git": true,
        "**/*assets*": true,
        "**/*zsh_history*": true,
        "**/bower_components": true,
        "**/node_modules": true,
        "**/vendor/**": true,
        "**/vendor/github.com/hurbcom": false,
        "**/venv/**": true,
    },
    "shellformat.effectLanguages": [
        "azcli",
        "dotenv",
        "gitignore",
        "hosts",
        "ignore",
        "jvmoptions",
        "properties",
        "shellscript",
        "spring-boot-properties",
    ],
    "todo-tree.highlights.customHighlight": {
        "FIXME": {
            "background": "#ff0000",
            "foreground": "#000000",
            "icon": "bug",
            "iconColour": "#ff0000",
            "opacity": 100,
            "type": "text",
        },
        "TODO": {
            "background": "#ffcc00",
            "foreground": "#000000",
            "icon": "alert",
            "iconColour": "#ffcc00",
            "opacity": 100,
            "type": "text",
        },
        "NOTE": {
            "background": "#33cc33",
            "foreground": "#000000",
            "icon": "megaphone",
            "iconColour": "#33cc33",
            "opacity": 100,
            "type": "text",
        }
    },
    "todo-tree.general.tags": [
        "[ ]",
        "[x]",
        "FIXME",
        "NOTE",
        "TODO",
    ],
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "bracketLens.mode": "auto",
    "breadcrumbs.enabled": true,
    "debug.inlineValues": "off",
    "diffEditor.codeLens": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "diffEditor.maxComputationTime": 0,
    "editor.autoClosingBrackets": "never",
    "editor.autoClosingQuotes": "never",
    "editor.bracketPairColorization.enabled": true,
    "editor.fontFamily": "Menlo, Fira Code, Hack, monospace, Roboto Mono, Cascadia Mono, JetBrains Mono, SF Mono, UbuntuMono Nerd Font Mono, Emoji",
    "editor.fontLigatures": "'ss01', 'ss03', 'ss04', 'ss05', 'ss06', 'zero', 'onum'",
    "editor.formatOnPaste": false,
    "editor.formatOnType": true,
    "editor.guides.bracketPairs": "active",
    "editor.inlineSuggest.enabled": true,
    "editor.largeFileOptimizations": true,
    "editor.minimap.enabled": false,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.renderControlCharacters": true,
    "editor.renderWhitespace": "none",
    "editor.snippetSuggestions": "top",
    "editor.suggest.localityBonus": false,
    "editor.wordBasedSuggestions": false,
    "editor.wordWrap": "on",
    "emojisense.unicodeCompletionsEnabled": true,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "extensions.ignoreRecommendations": true,
    "files.autoSave": "onFocusChange",
    "files.trimTrailingWhitespace": true,
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "go.autocompleteUnimportedPackages": true,
    "go.buildOnSave": "off",
    "go.coverageOptions": "showUncoveredCodeOnly",
    "go.coverOnSave": false,
    "go.coverShowCounts": true,
    "go.formatTool": "goimports",
    "go.gotoSymbol.includeGoroot": true,
    "go.gotoSymbol.includeImports": true,
    "go.lintTool": "golangci-lint",
    "go.testOnSave": false,
    "go.testTimeout": "1m",
    "go.toolsManagement.autoUpdate": true,
    "go.useCodeSnippetsOnFunctionSuggest": true,
    "go.useCodeSnippetsOnFunctionSuggestWithoutType": true,
    "go.useLanguageServer": true,
    "go.vetOnSave": "off",
    "intelephense.environment.phpVersion": "5.6.40",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "partialDiff.enableTelemetry": false,
    "python.envFile": "${workspaceFolder}/.env",
    "python.linting.enabled": true,
    "python.linting.flake8Enabled": true,
    "python.linting.pycodestyleEnabled": false,
    "python.linting.pylintEnabled": true,
    "python.testing.pytestEnabled": true,
    "python.testing.unittestEnabled": true,
    "redhat.telemetry.enabled": false,
    "dev.containers.workspaceMountConsistency": "delegated",
    "scm.providerCountBadge": "auto",
    "security.workspace.trust.untrustedFiles": "open",
    "telemetry.telemetryLevel": "off",
    "terminal.integrated.defaultProfile.linux": "zsh",
    "trailing-spaces.deleteModifiedLinesOnly": true,
    "trailing-spaces.liveMatching": false,
    "trailing-spaces.trimOnSave": true,
    "update.showReleaseNotes": false,
    "window.newWindowDimensions": "maximized",
    "window.restoreWindows": "none",
    "window.title": "${dirty}${separator}${rootName}${separator}vs",
    "window.titleSeparator": " - ",
    "workbench.editor.restoreViewState": false,
    "workbench.startupEditor": "newUntitledFile",
    "workbench.editor.enablePreview": false,
    "git.autofetch": true,
    "workbench.colorTheme": "SynthWave '84",
    "window.zoomLevel": -1,
    "shellcheck.exclude": [
        "SC1090",
        "SC2148",
        "SC1071",
        "SC2296"
    ],
}