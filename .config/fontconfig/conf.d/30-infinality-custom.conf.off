<?xml version='1.0'?>
<!DOCTYPE fontconfig SYSTEM 'fonts.dtd'>
<fontconfig>
    <match>
        <test name="family">
            <string>Droid Sans Mono Slashed</string>
        </test>
        <edit name="family" mode="assign" binding="strong">
            <string>Consolas</string>
        </edit>
    </match>
</fontconfig>