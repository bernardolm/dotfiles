# echo iterating
for f in $DOTFILES/zsh/functions/log_*; do
	# echo $f
	source_func "$f"
done
# echo done iterating
source_func $DOTFILES/zsh/functions/caller

function log() {
  local level="$1"

	local silenced_levels=("debug" "env")

	if [ ! "$SHELL_DEBUG" = "true" ]; then
		if [[ ${silenced_levels[@]} =~ $level ]]; then
			return
		fi
	fi

	shift

	eval "log_$level '$@'"
}
